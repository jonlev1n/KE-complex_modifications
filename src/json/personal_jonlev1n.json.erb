{
    "title": "personal @jonlev1n",
    "rules": [
        {
            "description": "Change Control + i/j/k/l to Arrows",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("j", ['control'], ['any']) %>,
                    "to": <%= to([["left_arrow"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("k", ['control'], ['any']) %>,
                    "to": <%= to([["down_arrow"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("i", ['control'], ['any']) %>,
                    "to": <%= to([["up_arrow"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("l", ['control'], ['any']) %>,
                    "to": <%= to([["right_arrow"]]) %>
                }
            ]
        },

        {
            "description": "Change Option + j/l to beginning/end of word",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("j", ['option'], ['any']) %>,
                    "to": <%= to([["left_arrow", 'left_option']]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("l", ['option'], ['any']) %>,
                    "to": <%= to([["right_arrow", 'left_option']]) %>
                }
            ]
        },

        {
            "description": "Change CMD + j/l to command left_arrow/right_arrow",
            "manipulators": [
                 {
                    "type": "basic",
                    "from": <%= from("j", ['command', 'control'], ['any']) %>,
                    "to": <%= to([["left_arrow", 'left_command']]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("l", ['command', 'control'], ['any']) %>,
                    "to": <%= to([["right_arrow", 'left_command']]) %>
                }
            ]
        },

        {
            "description": "Control and -/= to display brightness",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("hyphen", ['control'], []) %>,
                    "to": <%= to([["display_brightness_decrement"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("equal_sign", ['control'], []) %>,
                    "to": <%= to([["display_brightness_increment"]]) %>
                }
            ]
        },

        {
            "description": "Control and 1/2/3 to rewind/play_or_pause/fastforward",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("1", ['control'], []) %>,
                    "to": <%= to([["rewind"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("2", ['control'], []) %>,
                    "to": <%= to([["play_or_pause"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("3", ['control'], []) %>,
                    "to": <%= to([["fastforward"]]) %>
                }
            ]
        },

        {
            "description": "] and delete simultaneously become forward delete",
            "manipulators": [
               {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "close_bracket"
                            },
                            {
                                "key_code": "delete_or_backspace"
                            }
                        ],
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward"
                        }
                    ]
                }
            ]
        }, 
         {
            "description": "spacbar and i/j/k/l simultaneously become arrows",
            "manipulators": [
               {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "spacebar"
                            },
                            {
                                "key_code": "j"
                            }
                        ],
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "spacebar"
                            },
                            {
                                "key_code": "i"
                            }
                        ],
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "spacebar"
                            },
                            {
                                "key_code": "k"
                            }
                        ],
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "spacebar"
                            },
                            {
                                "key_code": "l"
                            }
                        ],
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow"
                        }
                    ]
                }
            ]
        }, 

         {
            "description": "holding down space remaps to left control",
            "manipulators": [
               {
                    "type": "basic",
                    "from": {
                        "key_code": "spacebar",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 150,
                        "basic.to_if_held_down_threshold_milliseconds": 150
                    },
                    "to_if_alone": [
                        {
                            "key_code": "spacebar"
                        }
                    ],
                    "to_if_held_down": [
                        {
                            "key_code": "left_control"
                        }
                    ]
                }
            ]
        }
    ]
}  